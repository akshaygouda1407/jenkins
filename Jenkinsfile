pipeline{
    agent any
    stages{
        stage('build'){
            steps{
               echo "running build stage........." 
            }
        }
        stage('test'){
            steps{
                echo "runnning test stage......."
            }
        }
        stage('deploy'){
            steps{
                echo "running deploy which is final stage....."
            }
        }
        
    }
}
